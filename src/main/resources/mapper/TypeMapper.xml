<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbh.dao.TypeMapper">
    <resultMap id="BaseResultMap" type="com.hbh.entity.Type">
        <id column="protypeid" jdbcType="VARCHAR" property="protypeid"/>
        <result column="typename" jdbcType="VARCHAR" property="typename"/>
    </resultMap>
    <sql id="Example_Where_Clause">
        <where>
            <foreach collection="oredCriteria" item="criteria" separator="or">
                <if test="criteria.valid">
                    <trim prefix="(" prefixOverrides="and" suffix=")">
                        <foreach collection="criteria.criteria" item="criterion">
                            <choose>
                                <when test="criterion.noValue">
                                    and ${criterion.condition}
                                </when>
                                <when test="criterion.singleValue">
                                    and ${criterion.condition} #{criterion.value}
                                </when>
                                <when test="criterion.betweenValue">
                                    and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                                </when>
                                <when test="criterion.listValue">
                                    and ${criterion.condition}
                                    <foreach close=")" collection="criterion.value" item="listItem" open="("
                                             separator=",">
                                        #{listItem}
                                    </foreach>
                                </when>
                            </choose>
                        </foreach>
                    </trim>
                </if>
            </foreach>
        </where>
    </sql>
    <sql id="Base_Column_List">
        protypeid, typename
    </sql>
    <select id="selectByExample" parameterType="com.hbh.entity.TypeExample" resultMap="BaseResultMap">
        select
        <if test="distinct">
            distinct
        </if>
        'false' as QUERYID,
        <include refid="Base_Column_List"/>
        from type
        <if test="_parameter != null">
            <include refid="Example_Where_Clause"/>
        </if>
        <if test="orderByClause != null">
            order by ${orderByClause}
        </if>
    </select>
    <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from type
        where protypeid = #{protypeid,jdbcType=VARCHAR}
    </select>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        delete
        from type
        where protypeid = #{protypeid,jdbcType=VARCHAR}
    </delete>
    <insert id="insert" parameterType="com.hbh.entity.Type">
        insert into type (protypeid, typename)
        values (#{protypeid,jdbcType=VARCHAR}, #{typename,jdbcType=VARCHAR})
    </insert>
    <update id="updateByPrimaryKey" parameterType="com.hbh.entity.Type">
        update type
        set typename = #{typename,jdbcType=VARCHAR}
        where protypeid = #{protypeid,jdbcType=VARCHAR}
    </update>
    <select id="getbyparams" resultMap="BaseResultMap">
        select * from type
        <where>
            <if test="protypeid!=null and protypeid!=''">
                and protypeid = #{protypeid,jdbcType=VARCHAR}
            </if>
            <if test="typename!=null and typename!=''">
                and typename like concat('%',#{typename},'%')
            </if>

        </where>
    </select>
</mapper>